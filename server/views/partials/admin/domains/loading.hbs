{{#unless table_domains}}
  {{#ifEquals table_domains.length 0}}
    <tr class="no-data">
      <td>
        {{t "admin.no_domains"}}
      </td>
    </tr>
  {{else}}
    <tr class="loading-placeholder">
      <td>
        {{> icons/spinner}}
        {{t "admin.loading_domains"}}
      </td>
    </tr>
  {{/ifEquals}}
{{/unless}}
